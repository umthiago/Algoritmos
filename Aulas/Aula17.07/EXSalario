#include <stdio.h>
int main(){
    int grupo;
    printf("Quantos integrantes tem no grupo");
    scanf("%d", &grupo);
    if (grupo == 2 || grupo == 4){
        printf("8 minutos por aluno");
        
    } else if (grupo == 3 || grupo == 5){
            printf("10 minutos por aluno")
        } 
        
    
    return 0;
}
